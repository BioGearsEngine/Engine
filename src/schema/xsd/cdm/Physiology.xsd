<!-- @brief Physiological systems of the body and the data associated with them
     @details Each system and its data to be populated by physiology modeling and simulations 
-->
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" targetNamespace="uri:/mil/tatrc/physiology/datamodel" xmlns="uri:/mil/tatrc/physiology/datamodel" elementFormDefault="qualified">  
    
    <xs:include schemaLocation="System.xsd"/>
    <xs:include schemaLocation="PatientNutrition.xsd"/>
    
    <xs:complexType name="PupillaryResponseData">
        <xs:complexContent>
            <xs:extension base="ObjectData">
                <xs:sequence>
                    <xs:element name="ReactivityModifier" type="ScalarNeg1To1Data" minOccurs="0"  maxOccurs="1"/><!--<< @brief Change in pupil reaction time -1 complete reduction/no response, 0 is normal, and 1 is the fastest reaction time. -->
                    <xs:element name="ShapeModifier" type="ScalarNeg1To1Data" minOccurs="0"  maxOccurs="1"/><!--<< @brief Percentage change in pupil shape from normal. Positive is vertical ovoid. Negative is horizontal ovoid. -->
                    <xs:element name="SizeModifier" type="ScalarNeg1To1Data" minOccurs="0"  maxOccurs="1"/><!--<< @brief Pupil size change from normal. -1 is fully constricted, 0 is no change, +1 is fully dilated. -->
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    
    <!-- @brief Provides clinician level data reflecting the composition of the blood.
         @details Blood Chemistry is complete analysis on the blood at any given point during the simulation.
         The blood chemistry system houses all of the blood concentrations and compositions needed to assess a patient's health. This system is under development
         and will be improved in future releases to include more substances that can provide clinician level details and assessments, such as a CBC and blood panel.
     -->
    <xs:complexType name="BloodChemistrySystemData">
        <xs:complexContent>
            <xs:extension base="SystemData">
                <xs:sequence>                     
                    <xs:element name="BloodDensity" type="ScalarMassPerVolumeData" minOccurs="0" maxOccurs="1"/><!--<< @brief Mass per unit volume of the blood. -->
                    <xs:element name="BloodPH" type="ScalarData" minOccurs="0"  maxOccurs="1"/><!--<< @brief Measure of acidity or basicity of the blood. -->
                    <xs:element name="BloodSpecificHeat" type="ScalarHeatCapacitancePerMassData" minOccurs="0" maxOccurs="1"/> <!--<< @brief The amount of energy required to raise one kilogram of blood by one degree Celsius. -->
                    <xs:element name="BloodUreaNitrogenConcentration" type="ScalarMassPerVolumeData" minOccurs="0" maxOccurs="1"/><!--<< @brief The BUN concentration in the blood.-->                    
                    <xs:element name="CarbonDioxideSaturation" type="ScalarFractionData" minOccurs="0"  maxOccurs="1"/><!--<< @brief The fraction of hemoglobin that is carbon dioxide bound.-->					
                    <xs:element name="CarbonMonoxideSaturation" type="ScalarFractionData" minOccurs="0"  maxOccurs="1"/><!--<< @brief The fraction of hemoglobin that is carbon monoxide bound.-->                    
                    <xs:element name="Hematocrit" type="ScalarFractionData" minOccurs="0"  maxOccurs="1"/><!--<< @brief Volume percentage of red blood cells in the blood.-->
                    <xs:element name="HemoglobinContent" type="ScalarMassData" minOccurs="0"  maxOccurs="1"/><!--<< @brief The total amount of hemoglobin in the blood.-->
                    <xs:element name="OxygenSaturation" type="ScalarFractionData" minOccurs="0"  maxOccurs="1"/><!--<< @brief The fraction of hemoglobin that is oxygen bound.-->
                    <xs:element name="Phosphate" type="ScalarAmountPerVolumeData" minOccurs="0"  maxOccurs="1"/><!--<< @brief Sum of all inorganic phosphate in the blood.-->
                    <xs:element name="PlasmaVolume" type="ScalarVolumeData" minOccurs="0"  maxOccurs="1"/><!--<< @brief Volume of the plasma.-->
                    <xs:element name="PulseOximetry" type="ScalarFractionData" minOccurs="0"  maxOccurs="1"/><!--<< @brief The fraction of hemoglobin with bound oxygen as measured optically, which can include Carbon Monoxide.-->
                    <xs:element name="RedBloodCellCount" type="ScalarAmountPerVolumeData" minOccurs="0"  maxOccurs="1"/><!--<< @brief Total number of red blood cells in the blood.-->
                    <xs:element name="ShuntFraction" type="ScalarFractionData" minOccurs="0"  maxOccurs="1"/><!--<< @brief Fraction of the blood flow that bypasses the alveoli; therefore, does not engage is gas exchange.-->
					<xs:element name="StrongIonDifference" type="ScalarAmountPerVolumeData" minOccurs="0"  maxOccurs="1"/><!--<< @brief Difference between the positive and negative strong ion concentrations in the blood.-->
                    <xs:element name="TotalProteinConcentration" type="ScalarMassPerVolumeData" minOccurs="0" maxOccurs="1"/><!--<< @brief The concentration of urea in the blood.-->
                    <xs:element name="VolumeFractionNeutralLipidInPlasma" type="ScalarFractionData" minOccurs="0"  maxOccurs="1"/><!--<< @brief Fraction of the plasma that is made up of neutral lipids.-->
                    <xs:element name="VolumeFractionNeutralPhospholipidInPlasma" type="ScalarFractionData" minOccurs="0"  maxOccurs="1"/><!--<< @brief Fraction of the plasma that is made up of neutral phospholipids.-->
                    <xs:element name="WhiteBloodCellCount" type="ScalarAmountPerVolumeData" minOccurs="0"  maxOccurs="1"/><!--<< @brief Total number of white blood cells in the blood.-->
                    <xs:element name="RedBloodCellAcetylcholinesterase" type="ScalarAmountPerVolumeData" minOccurs="0" maxOccurs="1"/> <!--<<@brief Concentration of acetylcholinesterase associated with red blood cells-->
                    <!-- Substance Pressures -->
                    <xs:element name="ArterialCarbonDioxidePressure" type="ScalarPressureData" minOccurs="0"  maxOccurs="1"/><!--<< @brief Partial pressure of carbon dioxide in the aorta.-->
                    <xs:element name="ArterialOxygenPressure" type="ScalarPressureData" minOccurs="0"  maxOccurs="1"/><!--<< @brief Partial pressure of oxygen in the aorta.-->
                    <xs:element name="PulmonaryArterialCarbonDioxidePressure" type="ScalarPressureData" minOccurs="0"  maxOccurs="1"/><!--<< @brief Partial pressure of carbon dioxide in the pulmonary arteries.-->
                    <xs:element name="PulmonaryArterialOxygenPressure" type="ScalarPressureData" minOccurs="0"  maxOccurs="1"/><!--<< @brief Partial pressure of oxygen in the pulmonary arteries.-->
                    <xs:element name="PulmonaryVenousCarbonDioxidePressure" type="ScalarPressureData" minOccurs="0"  maxOccurs="1"/><!--<< @brief Partial pressure of carbon dioxide in the pulmonary veins.-->
                    <xs:element name="PulmonaryVenousOxygenPressure" type="ScalarPressureData" minOccurs="0"  maxOccurs="1"/><!--<< @brief Partial pressure of oxygen in the pulmonary veins.-->
                    <xs:element name="VenousCarbonDioxidePressure" type="ScalarPressureData" minOccurs="0"  maxOccurs="1"/><!--<< @brief Partial pressure of carbon dioxide in the vena cava.-->				
                    <xs:element name="VenousOxygenPressure" type="ScalarPressureData" minOccurs="0"  maxOccurs="1"/><!--<< @brief Partial pressure of oxygen in the vena cava.-->                    
                </xs:sequence>            
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:element name="BloodChemistrySystem" type="BloodChemistrySystemData"/>
    
    <!-- @brief Available heart rhythms for use in systems. -->
	<xs:simpleType name="enumHeartRhythm">
		<xs:restriction base="xs:string">
			<xs:enumeration value="Asystole"/>
			<xs:enumeration value="NormalSinus"/>
		</xs:restriction>
	</xs:simpleType>
 
    <!-- @brief Provides a representation of the human vasculature.
         @details The heart is the driving force that creates blood flow through the cardiovascular system. 
         The resulting blood flow is utilized by other physiologic systems as a means of transporting the oxygen, carbon dioxide
         and other substances throughout the human body.
     -->
    <xs:complexType name="CardiovascularSystemData">
        <xs:complexContent>
            <xs:extension base="SystemData">
                <xs:sequence>
                    <xs:element name="ArterialPressure" type="ScalarPressureData" minOccurs="0"  maxOccurs="1"/> <!--<< @brief The current arteial pressure.-->
                    <xs:element name="BloodVolume" type="ScalarVolumeData" minOccurs="0"  maxOccurs="1"/> <!--<< @brief The total volume of fluid within the cardiovascular system.-->
                    <xs:element name="CardiacIndex" type="ScalarVolumePerTimeAreaData" minOccurs="0"  maxOccurs="1"/> <!--<< @brief Relates heart performance to the size of the individual.-->
                    <xs:element name="CardiacOutput" type="ScalarVolumePerTimeData" minOccurs="0"  maxOccurs="1"/> <!--<< @brief The volume of blood being pumped from the heart in a given time interval.<br> It is defined as the heart rate multiplied by the stroke volume.-->
                    <xs:element name="CentralVenousPressure" type="ScalarPressureData" minOccurs="0"  maxOccurs="1"/> <!--<< @brief The instantaneous pressure of the venous return.-->
                    <xs:element name="CerebralBloodFlow" type="ScalarVolumePerTimeData" minOccurs="0"  maxOccurs="1"/> <!--<< @brief The rate of blood flow into the skull, thus supplying the brain.-->
                    <xs:element name="CerebralPerfusionPressure" type="ScalarPressureData" minOccurs="0"  maxOccurs="1"/> <!--<< @brief The difference between the mean arterial pressure and the cerebral blood pressure. Defines the hydrostatic diffusion gradient.-->
                    <xs:element name="DiastolicArterialPressure" type="ScalarPressureData" minOccurs="0"  maxOccurs="1"/> <!--<< @brief The minimum pressure in the aorta over the course of a cardiac cycle.-->
                    <xs:element name="HeartEjectionFraction" type="ScalarFractionData" minOccurs="0"  maxOccurs="1"/> <!--<< @brief The current pressure in the aorta.-->
                    <xs:element name="HeartRate" type="ScalarFrequencyData" minOccurs="0"  maxOccurs="1"/> <!--<< @brief The fraction of the blood that is pumped out of the ventricle during a cardiac cycle.-->
                    <xs:element name="HeartRhythm" type="enumHeartRhythm" minOccurs="0"  maxOccurs="1"/> <!--<< @brief Current heart rhythm -->			
                    <xs:element name="HeartStrokeVolume" type="ScalarVolumeData" minOccurs="0"  maxOccurs="1"/> <!--<< @brief The volume of blood pumped from the left ventricle in one contraction.-->
                    <xs:element name="IntracranialPressure" type="ScalarPressureData" minOccurs="0"  maxOccurs="1"/> <!--<< @brief The pressure inside the skull. Equal to the cerebral blood pressure with the cerebrospinal fluid pressure being assumed equal.-->
                    <xs:element name="MeanArterialPressure" type="ScalarPressureData" minOccurs="0"  maxOccurs="1"/> <!--<< @brief The average arterial pressure throughout each cardiac cycle.-->
                    <xs:element name="MeanArterialCarbonDioxidePartialPressure" type="ScalarPressureData" minOccurs="0"  maxOccurs="1"/> <!--<< @brief The mean of the partial pressure of carbon dioxide in the aorta over a cardiac cycle.-->
                    <xs:element name="MeanArterialCarbonDioxidePartialPressureDelta" type="ScalarPressureData" minOccurs="0"  maxOccurs="1"/> <!--<< @brief The change of the the mean of the partial pressure of carbon dioxide in the aorta from the previous cardiac cycle.-->
                    <xs:element name="MeanCentralVenousPressure" type="ScalarPressureData" minOccurs="0"  maxOccurs="1"/> <!--<< @brief The average pressure in the vena cava over the course of a cardiac cycle.-->
                    <xs:element name="MeanSkinFlow" type="ScalarVolumePerTimeData" minOccurs="0"  maxOccurs="1"/> <!--<< @brief The blood flow to the skin  averaged over the current cardiac cycle time-->
                    <xs:element name="PulmonaryArterialPressure" type="ScalarPressureData" minOccurs="0"  maxOccurs="1"/> <!--<< @brief The current pressure in the pulmonary arteries.-->
                    <xs:element name="PulmonaryCapillariesWedgePressure" type="ScalarPressureData" minOccurs="0"  maxOccurs="1"/> <!--<< @brief The pressure within the pulmonary capillaries.-->
                    <xs:element name="PulmonaryDiastolicArterialPressure" type="ScalarPressureData" minOccurs="0"  maxOccurs="1"/> <!--<< @brief The minimum pressure that occurs in the pulmonary arteries over the course of a cardiac cycle.-->
                    <xs:element name="PulmonaryMeanArterialPressure" type="ScalarPressureData" minOccurs="0"  maxOccurs="1"/> <!--<< @brief The average pressure that occurs in the pulmonary arteries over the course of a cardiac cycle.-->
                    <xs:element name="PulmonaryMeanCapillaryFlow" type="ScalarVolumePerTimeData" minOccurs="0"  maxOccurs="1"/> <!--<< @brief The average blood flow in the pulmonary capillaries over the course of a cardiac cycle.-->
                    <xs:element name="PulmonaryMeanShuntFlow" type="ScalarVolumePerTimeData" minOccurs="0"  maxOccurs="1"/> <!--<< @brief The average blood flow diverted from the pulmonary capillaries (not oxygenated) over the course of a cardiac cycle.-->
                    <xs:element name="PulmonarySystolicArterialPressure" type="ScalarPressureData" minOccurs="0"  maxOccurs="1"/> <!--<< @brief The maximum pressure in the pulmonary arteries over the course of a cardiac cycle.-->
                    <xs:element name="PulmonaryVascularResistance" type="ScalarFlowResistanceData" minOccurs="0"  maxOccurs="1"/> <!--<< @brief The resistance offered by the pulmonary circulation.-->
                    <xs:element name="PulmonaryVascularResistanceIndex" type="ScalarPressureTimePerVolumeAreaData" minOccurs="0"  maxOccurs="1"/> <!--<< @brief Relates pulmonary circulation performance to the size of the individual. -->
                    <xs:element name="PulsePressure" type="ScalarPressureData" minOccurs="0"  maxOccurs="1"/> <!--<< @brief The difference between the systolic and diastolic pressures.-->
                    <xs:element name="SystemicVascularResistance" type="ScalarFlowResistanceData" minOccurs="0"  maxOccurs="1"/> <!--<< @brief The resistance to blood flow through the entire systemic vasculature, not including the pulmonary circulation.-->						
                    <xs:element name="SystolicArterialPressure" type="ScalarPressureData" minOccurs="0"  maxOccurs="1"/> <!--<< @brief The maximum pressure in the aorta over the course of a cardiac cycle.-->				                    
                </xs:sequence>            
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:element name="CardiovascularSystem" type="CardiovascularSystemData"/>
    
    <!-- @brief Provides transport of drugs through the blood and air in the cardiovascular and respiratory systems, respectively.
         @details The exchange of gases across the alveoli/pulmonary capillary barrier, and the diffusion of substances into and out of tissues. 
         Important substances required for cell function are transported to the organs, while waste material is transported away for expulsion from the body.
         Drugs impact physiologic behavior, such as heart rate, respiration rate, and blood pressure through modifiers.
         Totals are from all active drugs in the body.
     -->
    <xs:complexType name="DrugSystemData">
        <xs:complexContent>
            <xs:extension base="SystemData">
                <xs:sequence>
                    <xs:element name="BronchodilationLevel" type="ScalarFractionData" minOccurs="0"  maxOccurs="1"/> <!--<< @brief -->
                    <xs:element name="HeartRateChange" type="ScalarFrequencyData" minOccurs="0"  maxOccurs="1"/> <!--<< @brief -->
                    <xs:element name="MeanBloodPressureChange" type="ScalarPressureData" minOccurs="0"  maxOccurs="1"/> <!--<< @brief -->
                    <xs:element name="NeuromuscularBlockLevel" type="ScalarFractionData" minOccurs="0"  maxOccurs="1"/> <!--<< @brief -->
                    <xs:element name="PulsePressureChange" type="ScalarPressureData" minOccurs="0"  maxOccurs="1"/> <!--<< @brief -->
                    <xs:element name="PupillaryResponse" type="PupillaryResponseData" minOccurs="0"  maxOccurs="1"/> <!--<< @brief -->
                    <xs:element name="RespirationRateChange" type="ScalarFrequencyData" minOccurs="0"  maxOccurs="1"/> <!--<< @brief -->
                    <xs:element name="SedationLevel" type="ScalarFractionData" minOccurs="0"  maxOccurs="1"/> <!--<< @brief -->
                    <xs:element name="TidalVolumeChange" type="ScalarVolumeData" minOccurs="0"  maxOccurs="1"/> <!--<< @brief -->
                    <xs:element name="TubularPermeabilityChange" type="ScalarFractionData" minOccurs="0"  maxOccurs="1"/> <!--<< @brief -->
                    <xs:element name="CentralNervousResponse" type="ScalarFractionData" minOccurs="0"  maxOccurs="1"/> <!--<< @brief -->
                </xs:sequence>  
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:element name="DrugSystem" type="DrugSystemData"/>
    
    <!-- @brief Releases hormones to maintain homeostasis and regulate body functions, including metabolism and behavior.
         @details The release of the hormones in response to the stimuli to represent the response of the sympathetic nervous system.
     -->
    <xs:complexType name="EndocrineSystemData">
        <xs:complexContent>
            <xs:extension base="SystemData">
                <xs:sequence>
                    <xs:element name="InsulinSynthesisRate" type="ScalarAmountPerTimeData" minOccurs="0"  maxOccurs="1"/> <!--<< @brief Insulin production rate. -->
                    <xs:element name="GlucagonSynthesisRate" type="ScalarAmountPerTimeData" minOccurs="0"  maxOccurs="1"/> <!--<< @brief Glucagon production rate. -->
                </xs:sequence>	
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:element name="EndocrineSystem" type="EndocrineSystemData"/>
    
    <!-- @brief Provides heat transfer, nutrition, and exercise physiology in the human body.
     @details 
     -->
    <xs:complexType name="EnergySystemData">
        <xs:complexContent>
            <xs:extension base="SystemData">
                <xs:sequence>
                    <xs:element name="AchievedExerciseLevel" type="ScalarFractionData" minOccurs="0" maxOccurs="1"/><!--<< @brief The fraction of how much of the requested exercise the patient is able to achieve. 1.0 means the the patient is performing at the requested level. Nan is the patient is not exercising (no Action)-->
                    <xs:element name="CoreTemperature" type="ScalarTemperatureData" minOccurs="0" maxOccurs="1"/> <!--<< @brief The internal body temperature.-->
                    <xs:element name="CreatinineProductionRate" type="ScalarAmountPerTimeData" minOccurs="0" maxOccurs="1"/> <!--<< @brief The generation of creatinine in the muscle tissue per -->                    
                    <xs:element name="ExerciseMeanArterialPressureDelta" type="ScalarPressureData" minOccurs="0" maxOccurs="1"/><!--<< @brief The pressure delta that occurs during exercise. Used to modify the baroreceptor set-point -->
                    <xs:element name="FatigueLevel" type="ScalarFractionData" minOccurs="0" maxOccurs="1"/> <!--<< @brief The exhaustion level of the body, or inability to perform physical exercise @details The amount of energy available for activity divided by the normal rested energy -->                            
                    <xs:element name="LactateProductionRate" type="ScalarAmountPerTimeData" minOccurs="0" maxOccurs="1"/> <!--<< @brief The molar amount of lactate generated on the tissue level from anaerobic metabolism -->
                    <xs:element name="SkinTemperature" type="ScalarTemperatureData" minOccurs="0" maxOccurs="1"/> <!--<< @brief The skin surface temperature.-->
                    <xs:element name="SweatRate" type="ScalarMassPerTimeData" minOccurs="0" maxOccurs="1"/> <!--<< @brief Mass of sweat generated per unit time due to elevated core temperature -->
                    <xs:element name="TotalMetabolicRate" type="ScalarPowerData" minOccurs="0" maxOccurs="1"/> <!--<< @brief The basal metabolic rate plus additional energy expenditure rates due to activity.-->
                    <xs:element name="TotalWorkRateLevel" type="ScalarFractionData" minOccurs="0" maxOccurs="1"/> <!--<< @brief The current patient work rate above basal over the maximum work rate capable by the patient -->
                    <xs:element name="SodiumLostToSweat" type="ScalarMassData" minOccurs="0" maxOccurs="1"/> <!--<<@brief Tracks the total amount of sodium lost while sweating during exercise -->
                    <xs:element name="PotassiumLostToSweat" type="ScalarMassData" minOccurs="0" maxOccurs="1"/> <!--<<@brief Tracks the total amount of potassium lost while sweating during exercise --> 
                    <xs:element name="ChlorideLostToSweat" type="ScalarMassData" minOccurs="0" maxOccurs="1"/> <!--<<@brief Tracks the total amount of chloride lost while sweating during exercise -->                    
                </xs:sequence>    
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:element name="EnergySystemData" type="EnergySystemData"/>   
    
    
    <!-- @brief Provides nutrients to the body
         @details Balances out the fluid and substances lost through other functionalities such as urination, sweating, and water loss from breathing.
     -->
    <xs:complexType name="GastrointestinalSystemData">
        <xs:complexContent>
            <xs:extension base="SystemData">
                <xs:sequence>
                    <xs:element name="ChymeAbsorptionRate" type="ScalarVolumePerTimeData" minOccurs="0" maxOccurs="1"/> <!--<< @brief Rate of the absorption of nutrients from chyme into the blood stream-->
                    <xs:element name="StomachContents" type="NutritionData" minOccurs="0" maxOccurs="1"/> <!--<< @brief The contents in the stomach-->
                </xs:sequence>	
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:element name="GastrointestinalSystem" type="GastrointestinalSystemData"/>
    
    <!-- @brief Provides liver function by clearing and maintaining substances in the blood
         @details Carries blood from parts of the gastrointestinal (GI) tract to the liver. The products of digestion are processed in the liver.
     -->
    <xs:complexType name="HepaticSystemData">
        <xs:complexContent>
            <xs:extension base="SystemData">
                <xs:sequence>
                    <xs:element name="KetoneProductionRate" type="ScalarAmountPerTimeData" minOccurs="0" maxOccurs="1"/> <!--<< @brief The rate of ketone production in the liver -->
                    <xs:element name="HepaticGluconeogenesisRate" type="ScalarMassPerTimeData" minOccurs="0" maxOccurs="1"/> <!--<< @brief The rate of glucose production in the liver -->
                </xs:sequence>  
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:element name="HepaticSystem" type="HepaticSystemData"/>
    
    <!-- @brief 
         @details
     -->
    <xs:complexType name="NervousSystemData">
        <xs:complexContent>
            <xs:extension base="SystemData">
                <xs:sequence>
                    <xs:element name="BaroreceptorHeartRateScale" type="ScalarData" minOccurs="0" maxOccurs="1"/><!--<< @brief Heart rate multiplier used to scale the heart rate due to baroreceptor feedback-->
                    <xs:element name="BaroreceptorHeartElastanceScale" type="ScalarData" minOccurs="0" maxOccurs="1"/><!--<< @brief Heart elastance multiplier used to scale the heart rate due to baroreceptor feedback-->
                    <xs:element name="BaroreceptorResistanceScale" type="ScalarData" minOccurs="0" maxOccurs="1"/><!--<< @brief Cardiovascular resistance multiplier used to scale the heart rate due to baroreceptor feedback-->
                    <xs:element name="BaroreceptorComplianceScale" type="ScalarData" minOccurs="0" maxOccurs="1"/><!--<< @brief Cardiovascular compliance multiplier used to scale the heart rate due to baroreceptor feedback-->                    
                    <xs:element name="LeftEyePupillaryResponse" type="PupillaryResponseData" minOccurs="0"  maxOccurs="1"/> <!--<< @brief -->                    
                    <xs:element name="RightEyePupillaryResponse" type="PupillaryResponseData" minOccurs="0"  maxOccurs="1"/> <!--<< @brief -->                                      
					<xs:element name="ChemoreceptorHeartRateScale" type="ScalarData" minOccurs="0" maxOccurs="1"/><!--<< @brief Heart rate multiplier used to scale the heart rate due to baroreceptor feedback-->
					<xs:element name="ChemoreceptorHeartElastanceScale" type="ScalarData" minOccurs="0" maxOccurs="1"/><!--<< @brief Heart elastance multiplier used to scale the heart rate due to baroreceptor feedback-->
                </xs:sequence>  
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:element name="NervousSystem" type="NervousSystemData"/>

    <!-- @brief Provides kidney function by clearing and maintaining substances in the blood
         @details Maintain substance balance in the blood by removing and maintaining fluid and
         substances in the blood with behavior dependent upon factors such as dehydration and the presence of diuretics.
     -->
    <xs:complexType name="RenalSystemData">
        <xs:complexContent>
            <xs:extension base="SystemData">
                <xs:sequence>    
                    <xs:element name="FiltrationFraction" type="ScalarFractionData" minOccurs="0"  maxOccurs="1"/> <!--<< @brief Scalar used to compute reabsorption for substances-->
                    <xs:element name="GlomerularFiltrationRate" type="ScalarVolumePerTimeData" minOccurs="0"  maxOccurs="1"/> <!--<< @brief Flow rate of filtrate into the tubules -->
                    
                    <xs:element name="LeftAfferentArterioleResistance" type="ScalarFlowResistanceData" minOccurs="0"  maxOccurs="1"/> <!--<< @brief resistance to flow going into the glomerular capillaries in the left Kidney-->
                    <xs:element name="LeftBowmansCapsulesHydrostaticPressure" type="ScalarPressureData" minOccurs="0"  maxOccurs="1"/> <!--<< @brief Pressure generated from fluid in the bowmans capsules in the left Kidney -->
                    <xs:element name="LeftBowmansCapsulesOsmoticPressure" type="ScalarPressureData" minOccurs="0"  maxOccurs="1"/> <!--<< @brief  Pressure generated from substances in the bowmans capsules in the left Kidney-->
                    <xs:element name="LeftEfferentArterioleResistance" type="ScalarFlowResistanceData" minOccurs="0"  maxOccurs="1"/> <!--<< @brief Resistance to flow into the peritubular capillaries in the left Kidney -->
                    <xs:element name="LeftGlomerularCapillariesHydrostaticPressure" type="ScalarPressureData" minOccurs="0"  maxOccurs="1"/> <!--<< @brief Pressure generated from fluid in the glomerular capillaries in the left Kidney -->
                    <xs:element name="LeftGlomerularCapillariesOsmoticPressure" type="ScalarPressureData" minOccurs="0"  maxOccurs="1"/> <!--<< @brief Pressure generated from substances in the capillaries in the left Kidney -->
                    <xs:element name="LeftGlomerularFiltrationCoefficient" type="ScalarVolumePerTimePressureData" minOccurs="0"  maxOccurs="1"/> <!--<< @brief Calculated for each substance to determine ease of transport into the tubules in the left Kidney -->
                    <xs:element name="LeftGlomerularFiltrationRate" type="ScalarVolumePerTimeData" minOccurs="0"  maxOccurs="1"/> <!--<< @brief Flow rate of filtrate into the tubules in left Kidney -->
                    <xs:element name="LeftGlomerularFiltrationSurfaceArea" type="ScalarAreaData" minOccurs="0"  maxOccurs="1"/> <!--<< @brief Surface area of glomerular capillaries that experiance filtration in the left Kidney -->
                    <xs:element name="LeftGlomerularFluidPermeability" type="ScalarVolumePerTimePressureAreaData" minOccurs="0"  maxOccurs="1"/> <!--<< @brief Permeability coeffitient of the capillary tissue in the left Kidney -->
                    <xs:element name="LeftFiltrationFraction" type="ScalarFractionData" minOccurs="0"  maxOccurs="1"/> <!--<< @brief Scalar used to compute reabsorption for substances in left Kidney -->
                    <xs:element name="LeftNetFiltrationPressure" type="ScalarPressureData" minOccurs="0"  maxOccurs="1"/> <!--<< @brief Total pressure directed into the tubules in the left Kidney -->
                    <xs:element name="LeftNetReabsorptionPressure" type="ScalarPressureData" minOccurs="0"  maxOccurs="1"/> <!--<< @brief Total pressure directed into the peritubular capillaries in the left Kidney -->
                    <xs:element name="LeftPeritubularCapillariesHydrostaticPressure" type="ScalarPressureData" minOccurs="0"  maxOccurs="1"/> <!--<< @brief Pressure generated by the fluid in the peritubular capillaries in the left Kidney -->
                    <xs:element name="LeftPeritubularCapillariesOsmoticPressure" type="ScalarPressureData" minOccurs="0"  maxOccurs="1"/> <!--<< @brief Pressure generated by the substance concentration in the peritubular capillaries in the left Kidney-->
                    <xs:element name="LeftReabsorptionFiltrationCoefficient" type="ScalarVolumePerTimePressureData" minOccurs="0"  maxOccurs="1"/> <!--<< @brief Value which determines how much of a substance is reabsorbed into the blood stream in the left Kidney-->
                    <xs:element name="LeftReabsorptionRate" type="ScalarVolumePerTimeData" minOccurs="0"  maxOccurs="1"/> <!--<< @brief Rate at which fluid or a substance is transported into the peritubular capillaries in the left Kidney -->                    
					<xs:element name="LeftTubularOsmoticPressure" type="ScalarPressureData" minOccurs="0"  maxOccurs="1"/> <!--<< @brief Pressure generated by substances in the tubules in the left Kidney -->                    
                    <xs:element name="LeftTubularHydrostaticPressure" type="ScalarPressureData" minOccurs="0"  maxOccurs="1"/> <!--<< @brief Pressure generated by the fluid in the tubules in the left Kidney -->
                    <xs:element name="LeftTubularReabsorptionFiltrationSurfaceArea" type="ScalarAreaData" minOccurs="0"  maxOccurs="1"/> <!--<< @brief Surface ares of the tubules which is subject to fluid transport (reabsorption) in the left Kidney -->
                    <xs:element name="LeftTubularReabsorptionFluidPermeability" type="ScalarVolumePerTimePressureAreaData" minOccurs="0"  maxOccurs="1"/> <!--<< @brief Permeability of the tissue in the tubules, influencing resistance to flow into peritubular capillaries in the left Kidney  -->
                   
					<xs:element name="RenalBloodFlow" type="ScalarVolumePerTimeData" minOccurs="0"  maxOccurs="1"/> <!--<< @brief Blood flow into the kidney that is not filtered into the tubules -->
                    <xs:element name="RenalPlasmaFlow" type="ScalarVolumePerTimeData" minOccurs="0"  maxOccurs="1"/> <!--<< @brief Plasma flow into the kidney that is not filtered into the tubules -->                    
					<xs:element name="RenalVascularResistance" type="ScalarFlowResistanceData" minOccurs="0"  maxOccurs="1"/> <!--<< @brief Total resistance o f the kidney vasculature  -->
                    
                    <xs:element name="RightAfferentArterioleResistance" type="ScalarFlowResistanceData" minOccurs="0"  maxOccurs="1"/> <!--<< @brief resistance to flow going into the glomerular capillaries in the right Kidney -->
                    <xs:element name="RightBowmansCapsulesHydrostaticPressure" type="ScalarPressureData" minOccurs="0"  maxOccurs="1"/> <!--<< @brief Pressure generated from fluid in the bowmans capsules in the right Kidney  -->
                    <xs:element name="RightBowmansCapsulesOsmoticPressure" type="ScalarPressureData" minOccurs="0"  maxOccurs="1"/> <!--<< @brief Pressure generated from substances in the bowmans capsules in the right Kidney  -->
                    <xs:element name="RightEfferentArterioleResistance" type="ScalarFlowResistanceData" minOccurs="0"  maxOccurs="1"/> <!--<< @brief Resistance to flow into the peritubular capillaries in the left Kidney -->
                    <xs:element name="RightGlomerularCapillariesHydrostaticPressure" type="ScalarPressureData" minOccurs="0"  maxOccurs="1"/> <!--<< @brief Pressure generated from fluid in the glomerular capillaries in the left Kidney -->
                    <xs:element name="RightGlomerularCapillariesOsmoticPressure" type="ScalarPressureData" minOccurs="0"  maxOccurs="1"/> <!--<< @brief Pressure generated from substances in the capillaries in the left Kidney -->
                    <xs:element name="RightGlomerularFiltrationCoefficient" type="ScalarVolumePerTimePressureData" minOccurs="0"  maxOccurs="1"/> <!--<< @brief Calculated for each substance to determine ease of transport into the tubules in the left Kidney -->
                    <xs:element name="RightGlomerularFiltrationRate" type="ScalarVolumePerTimeData" minOccurs="0"  maxOccurs="1"/> <!--<< @brief Flow rate of filtrate into the tubules in left Kidney -->
                    <xs:element name="RightGlomerularFiltrationSurfaceArea" type="ScalarAreaData" minOccurs="0"  maxOccurs="1"/> <!--<< @brief Surface area of glomerular capillaries that experiance filtration in the left Kidney -->
                    <xs:element name="RightGlomerularFluidPermeability" type="ScalarVolumePerTimePressureAreaData" minOccurs="0"  maxOccurs="1"/> <!--<< @brief Permeability coeffitient of the capillary tissue in the left Kidney-->
                    <xs:element name="RightFiltrationFraction" type="ScalarFractionData" minOccurs="0"  maxOccurs="1"/> <!--<< @brief Scalar used to compute reabsorption for substances in left Kidney -->
                    <xs:element name="RightNetFiltrationPressure" type="ScalarPressureData" minOccurs="0"  maxOccurs="1"/> <!--<< @brief  Total pressure directed into the tubules in the left Kidney -->
                    <xs:element name="RightNetReabsorptionPressure" type="ScalarPressureData" minOccurs="0"  maxOccurs="1"/> <!--<< @brief Total pressure directed into the peritubular capillaries in the left Kidney -->
                    <xs:element name="RightPeritubularCapillariesHydrostaticPressure" type="ScalarPressureData" minOccurs="0"  maxOccurs="1"/> <!--<< @brief Pressure generated by the fluid in the peritubular capillaries in the left Kidney -->
                    <xs:element name="RightPeritubularCapillariesOsmoticPressure" type="ScalarPressureData" minOccurs="0"  maxOccurs="1"/> <!--<< @brief Pressure generated by the substance concentration in the peritubular capillaries in the left Kidney -->
                    <xs:element name="RightReabsorptionFiltrationCoefficient" type="ScalarVolumePerTimePressureData" minOccurs="0"  maxOccurs="1"/> <!--<< @brief Value which determines how much of a substance is reabsorbed into the blood stream in the left Kidney -->
                    <xs:element name="RightReabsorptionRate" type="ScalarVolumePerTimeData" minOccurs="0"  maxOccurs="1"/> <!--<< @brief Rate at which fluid or a substance is transported into the peritubular capillaries in the left Kidney -->
                    <xs:element name="RightTubularOsmoticPressure" type="ScalarPressureData" minOccurs="0"  maxOccurs="1"/> <!--<< @brief Pressure generated by substances in the tubules in the left Kidney  -->                    
                    <xs:element name="RightTubularHydrostaticPressure" type="ScalarPressureData" minOccurs="0"  maxOccurs="1"/> <!--<< @brief Pressure generated by the fluid in the tubules in the left Kidney -->                    					
					<xs:element name="RightTubularReabsorptionFiltrationSurfaceArea" type="ScalarAreaData" minOccurs="0"  maxOccurs="1"/> <!--<< @brief Surface ares of the tubules which is subject to fluid transport (reabsorption) in the left Kidney -->
                    <xs:element name="RightTubularReabsorptionFluidPermeability" type="ScalarVolumePerTimePressureAreaData" minOccurs="0"  maxOccurs="1"/> <!--<< @brief Permeability of the tissue in the tubules, influencing resistance to flow into peritubular capillaries in the left Kidney -->
                    
					<xs:element name="UrinationRate" type="ScalarVolumePerTimeData" minOccurs="0"  maxOccurs="1"/> <!--<< @brief Rate that fluid exits the bladder  --> 
                    <xs:element name="UrineOsmolality" type="ScalarOsmolalityData" minOccurs="0"  maxOccurs="1"/> <!--<< @brief The osmolar pressure contributions of the sodium, urea and glucose divided by the mass of the fluid -->
                    <xs:element name="UrineOsmolarity" type="ScalarOsmolarityData" minOccurs="0"  maxOccurs="1"/> <!--<< @brief The osmolar pressure contributions of the sodium, urea and glucose divided by the volume of the fluid -->
                    <xs:element name="UrineProductionRate" type="ScalarVolumePerTimeData" minOccurs="0"  maxOccurs="1"/> <!--<< @brief The rate of fluid flow to the bladder -->
                    <xs:element name="UrineSpecificGravity" type="ScalarData" minOccurs="0"  maxOccurs="1"/> <!--<< @brief The density of the fluid relative to water -->
                    <xs:element name="UrineVolume" type="ScalarVolumeData" minOccurs="0"  maxOccurs="1"/> <!--<< @brief The volume of the urine in the bladder --> 
                    <xs:element name="UrineUreaNitrogenConcentration" type="ScalarMassPerVolumeData" minOccurs="0"  maxOccurs="1"/> <!--<< @brief Scaled concentration of Urea in the bladder  --> 
                </xs:sequence>  
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>    
    <xs:element name="RenalSystem" type="RenalSystemData"/>
    
    <!-- @brief Provides ventilation and gas exchange of the human respiratory system.
         @details Supports changes in the lungs volume and pressure data during the flow
         and transfer of gases between the lungs, the blood and the outside environment.          
     -->
    <xs:complexType name="RespiratorySystemData">
        <xs:complexContent>
            <xs:extension base="SystemData">
                <xs:sequence>
                    <xs:element name="AlveolarArterialGradient" type="ScalarPressureData" minOccurs="0"  maxOccurs="1"/> <!--<< @brief Measure of the difference between the alveolar concentration of oxygen and the arterial concentration of oxygen to assess the integrity of the alveolar capillary unit..-->
                    <xs:element name="CarricoIndex" type="ScalarPressureData" minOccurs="0"  maxOccurs="1"/> <!--<< @brief The ratio of arterial partial pressure O2 and the inspired O2. O2 level in blood vs. Concentration of O2 in a breath.-->
                    <xs:element name="EndTidalCarbonDioxideFraction" type="ScalarFractionData" minOccurs="0"  maxOccurs="1"/> <!--<< @brief The fraction of CarbonDioxide in the trachea that is carbon dioxide at the end of each respiratory cycle.-->
                    <xs:element name="EndTidalCarbonDioxidePressure" type="ScalarPressureData" minOccurs="0"  maxOccurs="1"/> <!--<< @brief The pressure of CarbonDioxide in the trachea that is carbon dioxide at the end of each respiratory cycle.-->
                    <xs:element name="ExpiratoryFlow" type="ScalarVolumePerTimeData" minOccurs="0"  maxOccurs="1"/> <!--<< @brief Instantaneous airflow out of the lungs (negative value when inhaling)-->
                    <xs:element name="InspiratoryExpiratoryRatio" type="ScalarData" minOccurs="0"  maxOccurs="1"/> <!--<< @brief The ratio of the length of time of inspiration to the length of time of expiration.-->
                    <xs:element name="InspiratoryFlow" type="ScalarVolumePerTimeData" minOccurs="0"  maxOccurs="1"/> <!--<< @brief TInstantaneous airflow into the lungs (negative value when exhaling).-->
                    <xs:element name="PulmonaryCompliance" type="ScalarFlowComplianceData" minOccurs="0"  maxOccurs="1"/> <!--<< @brief The total compliance of the lungs.-->
                    <xs:element name="PulmonaryResistance" type="ScalarFlowResistanceData" minOccurs="0"  maxOccurs="1"/> <!--<< @brief The total resistance to airflow through the lungs.-->
                    <xs:element name="RespirationDriverPressure" type="ScalarPressureData" minOccurs="0"  maxOccurs="1"/> <!--<< @brief Pressure that drives the respiratory system in generating physiologic pressures.-->                    
                    <xs:element name="RespirationMusclePressure" type="ScalarPressureData" minOccurs="0"  maxOccurs="1"/> <!--<< @brief Combined driver pressure of the muscles that contribute to inhalation and exhalation, by aiding in the expansion and contraction of the thoracic cavity.-->                    
                    <xs:element name="RespirationRate" type="ScalarFrequencyData" minOccurs="0"  maxOccurs="1"/> <!--<< @brief The frequency of the respiratory cycle.-->	
                    <xs:element name="SpecificVentilation" type="ScalarData" minOccurs="0"  maxOccurs="1"/> <!--<< @brief The ratio of the volume of gas entering a region of the lung following an inspiration and the end-expiratory volume of that same lung region.-->
                    <xs:element name="TidalVolume" type="ScalarVolumeData" minOccurs="0"  maxOccurs="1"/> <!--<< @brief The volume of air moved into or out of the lungs during normal respiration.-->
                    <xs:element name="TotalAlveolarVentilation" type="ScalarVolumePerTimeData" minOccurs="0"  maxOccurs="1"/> <!--<< @brief The air flow to the alveoli.-->
                    <xs:element name="TotalDeadSpaceVentilation" type="ScalarVolumePerTimeData" minOccurs="0"  maxOccurs="1"/> <!--<< @brief The air flow to the dead space.-->
                    <xs:element name="TotalLungVolume" type="ScalarVolumeData" minOccurs="0"  maxOccurs="1"/> <!--<< @brief The total volume of air within the lungs at any given time.-->
                    <xs:element name="TotalPulmonaryVentilation" type="ScalarVolumePerTimeData" minOccurs="0"  maxOccurs="1"/> <!--<< @brief The flow rate representing the exchange of air between the lungs and ambient air.-->
                    <xs:element name="TranspulmonaryPressure" type="ScalarPressureData" minOccurs="0"  maxOccurs="1"/> <!--<< @brief The pressure difference between the alveoli and the intrapleural space.-->	
                </xs:sequence>	
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:element name="RespiratorySystem" type="RespiratorySystemData"/>
    
    <!-- @brief Provides data pertaining to the extracellular and intracelluar space          
     -->
    <xs:complexType name="TissueSystemData">
        <xs:complexContent>
            <xs:extension base="SystemData">
                <xs:sequence>
                    <xs:element name="CarbonDioxideProductionRate" type="ScalarVolumePerTimeData" minOccurs="0" maxOccurs="1"/> <!--<< @brief The carbon dioxide generated on the tissue level from aerobic metabolism-->
                    <xs:element name="DehydrationFraction" type="ScalarFractionData" minOccurs="0" maxOccurs="1"/> <!--<< @brief The fraction of body weight lost as fluid since initialization -->
                    <xs:element name="ExtracellularFluidVolume" type="ScalarVolumeData" minOccurs="0"  maxOccurs="1"/><!--<< @brief Volume of fluid outside of cells AND outside of the blood vessels-->
                    <xs:element name="ExtravascularFluidVolume" type="ScalarVolumeData" minOccurs="0"  maxOccurs="1"/><!--<< @brief Total fluid volume outside of the blood vessels. -->
                    <xs:element name="IntracellularFluidPH" type="ScalarData" minOccurs="0"  maxOccurs="1"/><!--<< @brief The acidity or alkalinity of the extravascular intercellular fluid.-->
                    <xs:element name="IntracellularFluidVolume" type="ScalarVolumeData" minOccurs="0"  maxOccurs="1"/><!--<< @brief Volume of fluid in the cells outside of the blood vessels. --> 
                    <xs:element name="TotalBodyFluidVolume" type="ScalarVolumeData" minOccurs="0"  maxOccurs="1"/><!--<< @brief Total fluid volume inside the body. -->                   
                    <xs:element name="OxygenConsumptionRate" type="ScalarVolumePerTimeData" minOccurs="0" maxOccurs="1"/> <!--<< @brief The oxygen consumed on the tissue level from aerobic metabolism-->
                    <xs:element name="RespiratoryExchangeRatio" type="ScalarData" minOccurs="0"  maxOccurs="1"/> <!--<< @brief The true ratio of carbon dioxide produced to oxygen consumed -->    
                    <xs:element name="LiverInsulinSetPoint" type="ScalarAmountPerVolumeData" minOccurs="0" maxOccurs="1"/> <!--<< @brief The insulin molarity in the liver vasculature after stabilization -->
                    <xs:element name="LiverGlucagonSetPoint" type="ScalarMassPerVolumeData" minOccurs="0" maxOccurs="1"/> <!--<< @brief The glucagon concentration in the liver vasculature after stabilization -->
                    <xs:element name="MuscleInsulinSetPoint" type="ScalarAmountPerVolumeData" minOccurs="0" maxOccurs="1"/> <!--<< @brief The insulin molarity in the muscle vasculature after stabilization -->
                    <xs:element name="MuscleGlucagonSetPoint" type="ScalarMassPerVolumeData" minOccurs="0" maxOccurs="1"/>  <!--<< @brief The glucagon concentration in the muscle vasculature after stabilization -->
                    <xs:element name="FatInsulinSetPoint" type="ScalarAmountPerVolumeData" minOccurs="0" maxOccurs="1"/>  <!--<< @brief The insulin molarity in the fat vasculature after stabilization -->
                    <xs:element name="FatGlucagonSetPoint" type="ScalarMassPerVolumeData" minOccurs="0" maxOccurs="1"/> <!--<< @brief The glucagon concentration in the fat vasculature after stabilization -->
                    <xs:element name="LiverGlycogen" type="ScalarMassData" minOccurs="0" maxOccurs="1"/>  <!--<< @brief The glucose stored as glycogen in the liver tissue. -->
                    <xs:element name="MuscleGlycogen" type="ScalarMassData" minOccurs="0" maxOccurs="1"/> <!--<< @brief The glucose stored as glycogen in the muscle tissue. -->
                    <xs:element name="StoredProtein" type="ScalarMassData" minOccurs="0" maxOccurs="1"/>  <!--<< @brief The amino acids stored as protein in the muscle tissue. -->
                    <xs:element name="StoredFat" type="ScalarMassData" minOccurs="0" maxOccurs="1"/>  <!--<< @brief The triacylglycerol stored in the fat tissue. -->
                </xs:sequence>	
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:element name="TissueSystem" type="TissueSystemData"/>

</xs:schema>

